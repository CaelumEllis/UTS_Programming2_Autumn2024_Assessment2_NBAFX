<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>

<VBox fx:controller="controller.RecordController" styleClass="background-1" alignment="CENTER" prefHeight="300.0" prefWidth="500.0" spacing="20" xmlns:fx="http://javafx.com/fxml/1">

    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>

    <Label text="Season Record" styleClass="label-h2"/>

    <TableView fx:id="recordTable" prefHeight="200.0" prefWidth="500.0">
        <columns>
            <TableColumn fx:id="roundColumn" text="Round"/>
            <TableColumn fx:id="gameColumn" text="Game"/>
            <TableColumn fx:id="winningTeamColumn" text="Winning Team"/>
            <TableColumn fx:id="losingTeamColumn" text="Losing Team"/>
        </columns>
    </TableView>

    <Button fx:id="closeButton" text="Close" onAction="#close"/>
</VBox>